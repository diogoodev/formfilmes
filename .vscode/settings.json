{
    "liveServer.settings.port": 5501,
    "extends": "stylelint-config-recommended",
    "rules": {
        "at-rule-no-unknown": [
            true,
            {
                "ignoreAtRules": [
                    "extends"
                ]
            }
        ],
        "block-no-empty": null,
        "unit-allowed-list": [
            "em",
            "rem",
            "s"
        ]
    }
}